                <TextView
                    android:id="@+id/tv_T20Test"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/iv_T20Test"
                    android:gravity="center_horizontal"
                    android:paddingLeft="@dimen/dashboard_img_text_padding_left"
                    android:paddingStart="@dimen/dashboard_img_text_padding_left"
                    android:paddingRight="@dimen/dashboard_img_text_padding_right"
                    android:paddingEnd="@dimen/dashboard_img_text_padding_right"
                    android:paddingBottom="@dimen/dashboard_img_text_padding_bottom"
