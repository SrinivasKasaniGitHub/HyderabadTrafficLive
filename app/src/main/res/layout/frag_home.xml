                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/iv_live_traffic"
                    android:layout_width="@dimen/dashboard_img_width"
                    android:layout_height="@dimen/dashboard_img_height"
                    android:layout_centerHorizontal="true"
                    android:layout_gravity="center"
                    android:contentDescription="@null"
                    android:paddingStart="@dimen/dashboard_img_padding_left"
                    android:paddingLeft="@dimen/dashboard_img_padding_left"
                    android:paddingTop="@dimen/dashboard_img_padding_top"
                    android:paddingEnd="@dimen/dashboard_img_padding_right"
                    android:paddingRight="@dimen/dashboard_img_padding_right"
                    android:paddingBottom="@dimen/dashboard_img_padding_bottom"
                    android:src="@drawable/ic_live_traffic" />

                <TextView
                    android:id="@+id/tv_live_traffic"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/iv_live_traffic"
                    android:gravity="center_horizontal"
                    android:paddingLeft="@dimen/dashboard_img_text_padding_left"
                    android:paddingStart="@dimen/dashboard_img_text_padding_left"
                    android:paddingRight="@dimen/dashboard_img_text_padding_right"
                    android:paddingEnd="@dimen/dashboard_img_text_padding_right"
                    android:paddingBottom="@dimen/dashboard_img_text_padding_bottom"
                    android:text="@string/live_traffic"
                    android:textColor="@color/colorAccentDark"
                    android:textSize="@dimen/dashboard_img_text_size" />

            </RelativeLayout>

        </android.support.v7.widget.CardView>

    </LinearLayout>

    <LinearLayout
