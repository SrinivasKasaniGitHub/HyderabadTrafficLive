                    android:gravity="center_horizontal"
                    android:paddingLeft="@dimen/dashboard_img_text_padding_left"
                    android:paddingStart="@dimen/dashboard_img_text_padding_left"
                    android:paddingRight="@dimen/dashboard_img_text_padding_right"
                    android:paddingEnd="@dimen/dashboard_img_text_padding_right"
                    android:paddingBottom="@dimen/dashboard_img_text_padding_bottom"
                    android:text="@string/public_complaints"
                    android:textColor="@color/colorAccentDark"
                    android:textSize="@dimen/dashboard_img_text_size" />

            </RelativeLayout>

        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="@dimen/dashboard_card_view_margin"
            android:layout_weight="1"
            android:foreground="?android:attr/selectableItemBackground"
            card_view:cardBackgroundColor="@color/cardview_light_background"
