                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/iv_public_info"
                    android:layout_width="@dimen/dashboard_img_width"
                    android:layout_height="@dimen/dashboard_img_height"
