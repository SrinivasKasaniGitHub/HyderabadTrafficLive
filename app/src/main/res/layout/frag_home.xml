                    android:paddingLeft="@dimen/dashboard_img_padding_left"
                    android:paddingTop="@dimen/dashboard_img_padding_top"
                    android:paddingEnd="@dimen/dashboard_img_padding_right"
                    android:paddingRight="@dimen/dashboard_img_padding_right"
                    android:paddingBottom="@dimen/dashboard_img_padding_bottom"
