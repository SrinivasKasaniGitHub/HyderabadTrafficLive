<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/horz_layout0"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        tools:ignore="UseCompoundDrawables">

        <ImageView
            android:id="@+id/header_image"
            android:layout_width="@dimen/dashboard_htp_logo_width"
            android:layout_height="@dimen/dashboard_htp_logo_height"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="@dimen/dashboard_htp_logo_margin_top"
            android:contentDescription="@null"
            android:src="@drawable/ic_logo" />

        <TextView
            android:id="@+id/tv_app_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_marginTop="@dimen/dashboard_htp_header_text_margin_top"
            android:paddingTop="@dimen/dashboard_htp_header_padding_top"
            android:paddingBottom="@dimen/dashboard_htp_header_padding_bottom"
            android:text="@string/hyderabad_traffic_live"
            android:textColor="@color/colorAccentDark"
            android:textSize="@dimen/dashboard_htp_header_text_size"
            android:textStyle="bold" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/horz_layout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/horz_layout0"
        android:layout_marginTop="0dp"
        android:layout_marginStart="@dimen/dashboard_horz_layout_margin_left"
        android:layout_marginLeft="@dimen/dashboard_horz_layout_margin_left"
        android:layout_marginEnd="@dimen/dashboard_horz_layout_margin_right"
        android:layout_marginRight="@dimen/dashboard_horz_layout_margin_right"
        android:gravity="center"
        android:orientation="horizontal"
        android:weightSum="3">

        <android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="@dimen/dashboard_card_view_margin"
            android:layout_weight="1"
            android:foreground="?android:attr/selectableItemBackground"
            card_view:cardBackgroundColor="@color/cardview_light_background"
            card_view:cardCornerRadius="@dimen/dashboard_card_view_corners">

            <RelativeLayout
                android:id="@+id/rel_public_interface"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/iv_public_interface"
                    android:layout_width="@dimen/dashboard_img_width"
                    android:layout_height="@dimen/dashboard_img_height"
                    android:layout_centerHorizontal="true"
                    android:layout_gravity="center"
                    android:contentDescription="@null"
                    android:paddingStart="@dimen/dashboard_img_padding_left"
                    android:paddingLeft="@dimen/dashboard_img_padding_left"
                    android:paddingTop="@dimen/dashboard_img_padding_top"
                    android:paddingEnd="@dimen/dashboard_img_padding_right"
                    android:paddingRight="@dimen/dashboard_img_padding_right"
                    android:paddingBottom="@dimen/dashboard_img_padding_bottom"
                    android:src="@drawable/ic_public_interface" />

                <TextView
                    android:id="@+id/tv_public_interface"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/iv_public_interface"
                    android:gravity="center_horizontal"
                    android:paddingLeft="@dimen/dashboard_img_text_padding_left"
                    android:paddingStart="@dimen/dashboard_img_text_padding_left"
                    android:paddingRight="@dimen/dashboard_img_text_padding_right"
                    android:paddingEnd="@dimen/dashboard_img_text_padding_right"
                    android:paddingBottom="@dimen/dashboard_img_text_padding_bottom"
                    android:text="@string/public_interface"
                    android:textColor="@color/colorAccentDark"
                    android:textSize="@dimen/dashboard_img_text_size" />

            </RelativeLayout>

        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="@dimen/dashboard_card_view_margin"
            android:layout_weight="1"
            android:foreground="?android:attr/selectableItemBackground"
            card_view:cardBackgroundColor="@color/cardview_light_background"
            card_view:cardCornerRadius="@dimen/dashboard_card_view_corners">

            <RelativeLayout
                android:id="@+id/rel_auto_fare_estimation"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/iv_auto_fare_estimation"
                    android:layout_width="@dimen/dashboard_img_width"
                    android:layout_height="@dimen/dashboard_img_height"
                    android:layout_centerHorizontal="true"
                    android:layout_gravity="center"
                    android:contentDescription="@null"
                    android:paddingStart="@dimen/dashboard_img_padding_left"
                    android:paddingLeft="@dimen/dashboard_img_padding_left"
                    android:paddingTop="@dimen/dashboard_img_padding_top"
                    android:paddingEnd="@dimen/dashboard_img_padding_right"
                    android:paddingRight="@dimen/dashboard_img_padding_right"
                    android:paddingBottom="@dimen/dashboard_img_padding_bottom"
                    android:src="@drawable/ic_autofare_estimation" />

                <TextView
                    android:id="@+id/tv_auto_fare_estimation"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/iv_auto_fare_estimation"
                    android:gravity="center_horizontal"
                    android:paddingLeft="@dimen/dashboard_img_text_padding_left"
                    android:paddingStart="@dimen/dashboard_img_text_padding_left"
                    android:paddingRight="@dimen/dashboard_img_text_padding_right"
                    android:paddingEnd="@dimen/dashboard_img_text_padding_right"
                    android:paddingBottom="@dimen/dashboard_img_text_padding_bottom"
                    android:text="@string/auto_fare_estimation"
                    android:textColor="@color/colorAccentDark"
                    android:textSize="@dimen/dashboard_img_text_size" />

            </RelativeLayout>

        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="@dimen/dashboard_card_view_margin"
            android:layout_weight="1"
            android:foreground="?android:attr/selectableItemBackground"
            card_view:cardBackgroundColor="@color/cardview_light_background"
            card_view:cardCornerRadius="@dimen/dashboard_card_view_corners">

            <RelativeLayout
